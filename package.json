{
  "name": "pretender-query-param-handler",
  "version": "1.1.0",
  "description": "Library to help with working with Pretender and Query Param heavy APIs",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:rwjblue/pretender-query-param-handler.git"
  },
  "license": "MIT",
  "author": "Robert Jackson <me@rwjblue.com>",
  "main": "addon/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:compatibility": "ember try:each",
    "test:main": "ember test"
  },
  "dependencies": {
    "ember-cli-babel": "^7.21.0",
    "validate-peer-dependencies": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.21.0-beta.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-try": "^1.4.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^8.9.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "pretender": "^3.4.3",
    "prettier": "^2.1.1",
    "qunit": "^2.11.2",
    "release-it": "^14.0.2",
    "release-it-lerna-changelog": "^2.4.0"
  },
  "peerDependencies": {
    "pretender": "^3.4.3"
  },
  "engines": {
    "node": "10.* || 12.* || >= 14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "main": "index.js"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
